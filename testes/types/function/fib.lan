
d2char n :: Int -> Char{
     if(n == 0){ return '0';}
     if(n == 1){ return '1';}
     if(n == 2){ return '2';}
     if(n == 3){ return '3';}
     if(n == 4){ return '4';}
     if(n == 5){ return '5';}
     if(n == 6){ return '6';}
     if(n == 7){ return '7';}
     if(n == 8){ return '8';}
     if(n == 9){ return '9';}

}

cahr2d c :: Char -> Int{
     if(c == '0'){ return 0;}
     if(c == '1'){ return 1;}
     if(c == '2'){ return 2;}
     if(c == '3'){ return 3;}
     if(c == '4'){ return 4;}
     if(c == '5'){ return 5;}
     if(c == '6'){ return 6;}
     if(c == '7'){ return 7;}
     if(c == '8'){ return 8;}
     if(c == '9'){ return 9;}

}

bool2Str b :: Bool -> Char{
    if(b){ return 'T';}
    return 'F';

}

divide n pos buff :: Int -> Int -> Char[] -> Int{
    if(n == 0){ return pos+1;}
    buff[pos] = d2char(n % 10)[0];
    return divide(n / 10, pos - 1, buff)[0];
}

int2Str n vsz buff :: Int -> Int -> Char[] -> Int {
     i = vsz - 1;
     if(n == 0){
         buff[i] = '0';
         return i;
     }
     neg = n < 0;
     if(neg){
        n = n * (0 - 1);
     }
     return divide(n, i, buff)[0];
}

str2int b acc pos limit :: Char[] -> Int -> Int -> Int -> Int{
    if( pos < limit && !(b[pos] == ' ')){
        return str2int(b, 10*acc + cahr2d(b[pos])[0],  pos +1 , limit)[0];
    }
    return acc;
}


fibonacci n :: Int -> Int {
  if (n < 1)
    return n;
  if (n == 1)
    return n;
  return fibonacci(n-1)[0] + fibonacci(n-2)[0];
}

