
data Ponto{
    x :: Int;
    y :: Int;
}

main :: Void {
    x = 0;
    y = 0;
    b1 = equal(x,y)[0];

    p1 = new Ponto;
    p2 = new Ponto;
    p1.x  = x+1;
    p1.y =  y+1;
    p2.x = x + 2;
    p2.y = y + 2;

    b2 = equal(p1, p2)[0];

    if(b1) print('T'); else print('F');
    print('\n');
    if(b2) print('T'); else print('F');

}

class Eq a {
    equal :: a -> a -> Bool;
}

instance Eq for Int {
    equal x y :: Int -> Int -> Bool{
        return x == y;
    }
}


instance Eq for Ponto {
    equal p1 p2 :: Ponto -> Ponto -> Bool{
        return (p1.x == p2.x) && (p1.y == p2.y);
    }
}
